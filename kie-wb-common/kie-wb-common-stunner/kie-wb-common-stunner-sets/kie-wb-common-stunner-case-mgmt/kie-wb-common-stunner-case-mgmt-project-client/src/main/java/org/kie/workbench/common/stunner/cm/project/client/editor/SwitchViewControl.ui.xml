<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui">

  <g:HorizontalPanel ui:field="switchViewPanel">

    <g:cell>
      <g:HTMLPanel>
        <b:Tooltip ui:field="caseViewTooltip">
          <b:Button ui:field="caseViewButton" height="100%"/>
        </b:Tooltip>
      </g:HTMLPanel>
    </g:cell>

    <g:cell>
      <g:HTMLPanel>
        <b:Tooltip ui:field="processViewTooltip">
          <b:Button ui:field="processViewButton" height="100%"/>
        </b:Tooltip>
      </g:HTMLPanel>
    </g:cell>

  </g:HorizontalPanel>

</ui:UiBinder>