<jboss-deployment-structure
	xmlns="urn:jboss:deployment-structure:1.1">
	<deployment>
		<dependencies>
			<module name="javax.activation.api" />
			<module name="javax.persistence.api" />
			<module name="javax.servlet.api" />
			<module name="javax.transaction.api" />
